{
    "name": "@tamago-corp/material-symbols-react",
    "version": "0.1.0",
    "description": "Material Symbols as tree-shakable React components",
    "license": "Apache-2.0",
    "author": "Your Name",
    "sideEffects": false,
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "peerDependencies": {
        "react": ">=18"
    },
    "devDependencies": {
        "typescript": "^5.3.3",
        "@svgr/core": "^8.1.0",
        "@types/react": "^18.2.0",
        "fast-glob": "^3.3.2",
        "fs-extra": "^11.2.0",
        "tsup": "^8.0.1"
    },
    "scripts": {
        "generate": "node scripts/generate-icons.js",
        "generate:index": "node scripts/generate-index.js",
        "generate:all": "pnpm generate && pnpm generate:index",
        "build": "tsup",
        "release": "pnpm build && pnpm publish",
        "clean": "rm -rf dist"
    },
    "publishConfig": {
        "access": "public"
    }
}